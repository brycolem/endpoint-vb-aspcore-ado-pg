{
  "Kestrel": {
    "Limits": {
      "MaxConcurrentConnections": 10000,
      "MaxConcurrentUpgradedConnections": 500,
      "MaxRequestBodySize": 104857600,
      "RequestQueueLimit": 10000,
      "MinRequestBodyDataRate": {
        "BytesPerSecond": 240,
        "GracePeriod": "10s"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Error",
      "Microsoft.AspNetCore": "Error",
      "Microsoft": "Error"
    }
  },
  "AllowedHosts": "*"
}
